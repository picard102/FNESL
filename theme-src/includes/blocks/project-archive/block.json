{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "fnesl/project-archive",
  "title": "Project Archive",
  "category": "fnesl",
  "icon": "screenoptions",
  "description": "Grid of Projects with taxonomy filters.",
  "attributes": {
    "showFilters": {
      "type": "array",
      "default": ["expertise", "partners", "location", "client", "awards"]
    },
    "filterMode": {
      "type": "string",
      "default": "and"
    },
    "heading": {
      "type": "string",
      "default": "Projects"
    }
  },
  "supports": {
    "align": ["wide", "full"],
    "html": false
  },
  "editorScript": "file:./index.js",
  "style": "file:./style.css",
  "editorStyle": "file:./editor.css",
  "script": "file:./frontend.js",
  "render": "file:./render.php"
}
