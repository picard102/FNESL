{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "color": {
      "palette": [
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#004080"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#e63946"
        },
        {
          "name": "Neutral",
          "slug": "neutral",
          "color": "#f1f1f1"
        }
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "system-ui, sans-serif",
          "name": "System UI",
          "slug": "system"
        },
        {
          "fontFamily": "'Merriweather', serif",
          "name": "Merriweather",
          "slug": "merriweather"
        }
      ],
      "fontSizes": [
        { "slug": "small", "name": "Small", "size": "14px" },
        { "slug": "medium", "name": "Medium", "size": "18px" },
        { "slug": "large", "name": "Large", "size": "24px" },
        { "slug": "x-large", "name": "X-Large", "size": "32px" }
      ]
    },
    "spacing": {
      "units": ["px", "em", "rem", "%", "vh", "vw"],
      "spacingSizes": [
        { "slug": "small", "name": "Small", "size": "8px" },
        { "slug": "medium", "name": "Medium", "size": "16px" },
        { "slug": "large", "name": "Large", "size": "32px" }
      ]
    }
  },
  "styles": {
    "color": {
      "background": "#ffffff",
      "text": "#111111"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--system)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": 1.6
    }
  },
  "customTemplates": [],
  "templateParts": []
}
